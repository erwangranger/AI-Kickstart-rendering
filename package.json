{
    "name": "red-hat-kickstarts-app",
    "version": "0.1.0",
    "private": true,
    "description": "A showcase of AI kickstart examples for Red Hat OpenShift AI",
    "homepage": ".",
    "repository": {
        "type": "git",
        "url": "https://github.com/erwangranger/AI-Kickstart-rendering"
    },
    "dependencies": {
        "jsdom": "^24.0.0",
        "node-fetch": "^2.7.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "generate-static": "node scripts/generate-static.js",
        "clean": "rimraf functions api",
        "prebuild": "npm run clean && npm run generate-static",
        "setup": "npm run clean && rm -rf node_modules && npm cache clean --force && npm ci",
        "build:ci": "NODE_ENV=production npm_config_loglevel=verbose npm run build",
        "dev": "npm run setup && npm start",
        "build:local": "npm run setup && npm run build:ci",
        "postinstall": "node -e \"try { require('fs').statSync('.nvmrc') } catch (e) { console.log('⚠️  No .nvmrc file found. Please install Node.js 20.x') }\""
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-transform-class-properties": "^7.23.4",
        "@babel/plugin-transform-nullish-coalescing-operator": "^7.23.4",
        "@babel/plugin-transform-numeric-separator": "^7.23.4",
        "@babel/plugin-transform-optional-chaining": "^7.23.4",
        "@babel/plugin-transform-private-methods": "^7.23.4",
        "@babel/plugin-transform-private-property-in-object": "^7.23.4",
        "glob": "^10.3.10",
        "react-scripts": "5.0.1",
        "rimraf": "^5.0.5",
        "svgo": "^3.2.0",
        "typescript": "4.9.5"
    }
}
